#for <variable> in range(<number of times>):
    #<statement>


for count in range(3):      #print Hello , goodbye 3 times
    print('Hello!') 
    print('goodbye')


for l in range(4,1,-1):
    print(l, end='')


print("\n")

for g in range(3):
    num= eval(input("Enter Any Number : "))
    print("The Squared of the number :" +num*num)






#WHILE LOOP

for t in range(11):
    print(t)

y=0
while y<=10:
    print(y)
    y=y+1


#BREAK
    #The break statement can be used to break out of a for or while loop before the loop is finished

